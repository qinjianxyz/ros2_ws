#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ucsd-robocar-actuator2-pkg==0.0.0','console_scripts','adafruit_twist_node'
__requires__ = 'ucsd-robocar-actuator2-pkg==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ucsd-robocar-actuator2-pkg==0.0.0', 'console_scripts', 'adafruit_twist_node')()
    )
